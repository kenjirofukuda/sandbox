Object subclass: #GdsLog	instanceVariableNames: 'logStream category'	classVariableNames: ''	poolDictionaries: ''	category: 'GdsFeel-Developer-Debug'!!GdsLog methodsFor: 'as yet unclassified' stamp: 'kf 10/21/2009 14:55'!beginLog	logStream := WriteStream on: ''.! !!GdsLog methodsFor: 'as yet unclassified' stamp: 'kf 10/21/2009 14:57'!category	^category! !!GdsLog methodsFor: 'as yet unclassified' stamp: 'kf 10/21/2009 14:56'!category: aCategory	category := aCategory.! !!GdsLog methodsFor: 'as yet unclassified' stamp: 'kf 10/21/2009 14:57'!endLog	logStream ifNil: [^self].	logStream position = 0 ifTrue: [^self].	(StringHolder new contents: logStream contents)		openLabel: self category asString.! !!GdsLog methodsFor: 'as yet unclassified' stamp: 'kf 10/21/2009 14:58'!println: anObject	logStream cr.	logStream nextPutAll: anObject asString.	logStream endEntry! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!GdsLog class	instanceVariableNames: ''!